_p=upwork

pkgname=${_p}
pkgver=4.0.86.0
pkgrel=1
pkgdesc="Upwork Team App."
arch=('x86_64')
url="https://www.upwork.com/downloads/new-team-application"
license=('unknown')
groups=()
depends=('gtk2' 'gtkglext' 'libgcrypt15')
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=(!emptydirs)
install=
source=("http://updates.team.odesk.com/binaries/v4_0_86_0_ba6c68c617adc98/upwork_amd64.deb")
sha256sums=('6ebce0137df4e93f57135a8e4cf9576200acadb4e93e9b24a2275e8baf6cd1dc')

prepare() {
	cd "${srcdir}"
	tar -zxf "${srcdir}/data.tar.gz"
}

package() {
	cd "${srcdir}"
	cp -rp usr "${pkgdir}/usr"
}
